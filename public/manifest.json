{
    "manifest_version": 3,
    "name": "chrome-extension-boilerplate",
    "version": "0.0.0",
    "permissions": [
        "contextMenus"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "128": "icon_128.svg",
        "48": "icon_48.svg",
        "16": "icon_16.svg"
    },
    "action": {
        "default_icon": {
        },
        "default_title": "chrome-extension-boilerplate",
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    }
}
